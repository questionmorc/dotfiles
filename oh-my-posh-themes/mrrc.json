{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#8AADF4",
    "flamingo": "#f2cdcd",
    "green": "#a6e3a1",
    "lavender": "#b4befe",
    "os": "#ACB0BE",
    "peach": "#fab387",
    "red": "#f38ba8",
    "text": "#494D64",
    "wight": "#FFFFFF"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:green",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .SSHSession }}{{ .HostName }}{{ end }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b0",
          "background": "p:red",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_full"
          },
          "template": " {{ .Path }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b0",
          "background": "p:peach",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " {{ .HEAD }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b0",
          "background": "p:lavender",
          "type": "git",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
